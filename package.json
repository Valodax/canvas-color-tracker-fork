{
  "name": "canvas-color-tracker",
  "version": "1.0.0",
  "description": "A utility to track objects on a canvas by unique px color",
  "main": "dist/canvas-color-tracker.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vasturiano/canvas-color-tracker.git"
  },
  "keywords": [
    "canvas",
    "color",
    "tracker",
    "interaction",
    "hover",
    "hidden"
  ],
  "author": {
    "name": "Vasco Asturiano",
    "url": "http://bl.ocks.org/vasturiano"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vasturiano/canvas-color-tracker/issues"
  },
  "homepage": "https://github.com/vasturiano/canvas-color-tracker",
  "scripts": {
    "build": "rimraf dist && mkdir dist && rollup -c",
    "watch": "rollup -c -w",
    "minify": "uglifyjs dist/canvas-color-tracker.js -o dist/canvas-color-tracker.min.js -c -m --comments '/Version/'",
    "prepublish": "npm run build && npm run minify"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.63.2",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-watch": "^4.3.1",
    "uglify-js": "3.4.5"
  }
}
